<%= stylesheet_link_tag "partial", "data-turbo-track": "reload" %>
<% img_src = "https://www.pngkey.com/png/full/332-3321462_mobile-menu-for-barefoot-resort-vacations-hamburger-menu.png" %>
<% path = '/' %>
<% if back_view %>
  <% img_src = image_url('left-arrow.png') %>
  <% path = '/' %>
<% end %>
<div class="header-wrapper">
  <div class="main-header"> 
    <a href=<%= path %> class='ham-menu'>
      <img src=<%= img_src %> alt='menu-icon' class="icon-img" />
    </a>
    <h2 class="view-name"> <%= view_name %> </h2>
    <img src="data:image/svg+xml;base64," class="search-icon" alt="search-icon" />
  </div>
  <% if sort %>
  <div class="optional-part">
    <div class ="sort-icon">
      <p class="sort-type recent">Most recent</p>
      <p class="sort-type ancient">Most ancient </p>
    </div>
    
  </div>
  <% end %>
</div>