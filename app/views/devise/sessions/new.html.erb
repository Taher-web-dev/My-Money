
<div class="wrapper">
  <div class="header-design">
    
    <img src="https://img.icons8.com/color/48/000000/user-menu-male--v1.png" class='img-profile' />
    <h2> Welcome to My Money app!</h2>
  </div>
  <div class="operation">
    <%= link_to "Log In", new_user_session_path, class:'disabled-link' %>
    <%= link_to "Sign Up", new_user_registration_path, class:'active-link' %>
  </div>

<%= render "flash/flash" %>

<h2 class='log-in-title'>Log in</h2>

<%= form_for(resource, as: resource_name,class:'form-data' ,url: session_path(resource_name)) do |f| %>
  <div class="field">
    <div class="wrapper-data-icon">
      <img src="data:image/png;base64," class='data-icon'/>
    </div>
    <%= f.email_field :email, autofocus: true, autocomplete: "email",class: 'input-data', placeholder:'example@gmail.com' %>
  </div>

  <div class="field">
    <div class="wrapper-data-icon"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRawTBDi37gsxIvuvomIfkq2mGfnYfFc-U2lw&usqp=CAU" class='data-icon'/></div>
    <%= f.password_field :password, autocomplete: "current-password", class: 'input-data', placeholder:'Your Password' %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field-remember">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="sign-up">
    <%= f.submit "Log in", class:'sign-up-btn' %>
  </div>
<% end %>

<%= render "devise/shared/links", class:'useful-link' %>
</div>